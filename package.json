{
  "name": "pdf-ocr-2.4",
  "private": false,
  "version": "2.4.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "scan:secrets": "node scripts/secret-scan.js",
    "start:all": "cmd /c scripts\\start-all.bat",
    "docling:up": "powershell -ExecutionPolicy Bypass -NoProfile -File \"scripts\\run-docling-detached.ps1\" -BindHost 127.0.0.1 -BindPort 8008",
    "docling:down": "powershell -ExecutionPolicy Bypass -NoProfile -File \"scripts\\stop-docling.ps1\"",
    "docling:watch": "powershell -ExecutionPolicy Bypass -NoProfile -File \"scripts\\docling-watchdog.ps1\" -VenvPath C:\\docling-venv -IntervalSeconds 60"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "@google/genai": "^1.19.0",
    "marked": "^16.2.1",
    "pdfjs-dist": "4.4.168"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "tsx": "^4.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RafaelBritoChoco/pdf-ocr-pro.git"
  },
  "license": "MIT",
  "keywords": ["pdf", "ocr", "gemini", "openrouter", "ia", "estruturação", "extracao"],
  "engines": {"node": ">=18"}
}
